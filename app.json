{
  "expo": {
    "name": "BitClover",
    "slug": "bit-clover",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./src/assets/notification-icon.png",
          "color": "#000000"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.pdob.bitclover",
      "googleServicesFile": "./src/google-services.json",
      "useNextNotificationsApi": true,
      "icon": "./src/assets/notification-icon.png",
      "versionCode": 2
    },
    "notification": {
      "icon": "./src/assets/notification-icon.png",
      "color": "#ffffff",
      "androidMode": "default"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
